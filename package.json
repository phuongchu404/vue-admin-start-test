{
  "name": "vue3-admin-starter",
  "version": "2.0.1",
  "description": "A vue admin template powered by vite + typescript + vue3 + vue-router + pinia + element-plus + axios.",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "private": true,
  "type": "module",
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "admin-template",
    "element-plus",
    "pinia",
    "typescript",
    "vue-router",
    "axios"
  ],
  "homepage": "https://github.com/yisibell/vue3-admin-starter",
  "bugs": {
    "url": "https://github.com/yisibell/vue3-admin-starter/issues"
  },
  "scripts": {
    "dev": "vite",
    "build:ft": "vite build --mode ft",
    "build:prod": "vite build --mode production",
    "build:ft:type-check": "run-p type-check build:ft",
    "build:prod:type-check": "run-p type-check build:prod",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "release": "changelogen --release && git push --follow-tags"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@logicflow/core": "^1.2.18",
    "@logicflow/extension": "^1.2.19",
    "axios": "^1.6.5",
    "element-plus": "^2.5.1",
    "path-to-regexp": "^6.2.1",
    "pathe": "^1.1.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "toolkitcss": "^2.2.0",
    "trickling": "^1.14.0",
    "vue": "^3.4.14",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/fa": "^1.1.8",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.8",
    "@types/qs": "^6.9.11",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "changelogen": "^0.5.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^21.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-svg4vue": "^3.1.2",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18"
  }
}